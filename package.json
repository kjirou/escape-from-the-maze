  },
  "dependencies": {
    "babel": "5.8.21",
    "blessed": "0.1.21",
    "chalk": "1.1.1",
    "dev-null": "0.1.1",
    "flux": "2.0.3",
    "generate-maze-by-clustering": "0.0.4",
    "keymirror": "0.1.1",
    "keypress": "0.2.1",
    "lodash": "3.10.1",
    "minimist": "1.2.0",
    "react": "0.14.0-beta3",
    "react-blessed": "0.0.2",
    "request": "2.61.0",
    "rx": "3.0.0",
    "underscore.string": "3.1.1",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "eslint": "1.1.0",
    "espower-babel": "3.2.0",
    "fixpack": "2.2.0",
