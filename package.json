    "power-assert": "0.11.0",
    "sinon": "1.16.0",
    "validator": "4.0.3"
  },
  "engines": {
    "node": "<=0.12",
    "npm": "<=2"
  },
  "homepage": "https://github.com/kjirou/escape-from-the-maze#readme",
  "keywords": [
    "cui",
    "game",
    "maze"
