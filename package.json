{
  "name": "escape-from-the-maze",
  "description": "A simple & tiny CUI maze game",
  "version": "0.0.0",
  "author": "kjirou",
  "bin": {
    "escape-from-the-maze": "./bin/escape-from-the-maze"
  },
  "bugs": {
    "url": "https://github.com/kjirou/escape-from-the-maze/issues"
  },
  "dependencies": {
    "babel": "5.8.21",
    "blessed": "0.1.21",
    "bluebird": "2.9.34",
    "chalk": "1.1.1",
    "dev-null": "0.1.1",
    "flux": "2.0.3",
    "generate-maze-by-clustering": "0.0.4",
    "keymirror": "0.1.1",
    "lodash": "3.10.1",
    "object-assign": "^4.0.1",
    "react": "0.13.3",
    "rx": "3.0.0",
    "underscore.string": "3.1.1",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "babelify": "6.2.0",
    "browserify": "11.0.1",
    "eslint": "1.1.0",
    "espower-babel": "3.2.0",
    "findit": "2.0.0",
    "fixpack": "2.2.0",
    "gulp": "3.9.0",
    "gulp-concat": "2.6.0",
    "gulp-rename": "1.2.2",
    "gulp-stylus": "2.0.6",
    "http-server": "0.8.0",
    "licensify": "1.4.0",
    "mocha": "2.2.5",
    "nib": "1.1.0",
    "node-notifier": "4.2.3",
    "power-assert": "0.11.0",
    "sinon": "1.16.0",
    "stylus": "0.52.0",
    "testem": "0.9.2",
    "validator": "4.0.3",
    "vinyl-source-stream": "1.1.0",
    "vinyl-transform": "1.0.0"
  },
  "engines": {
    "node": "<=0.12",
    "npm": "<=2"
  },
  "homepage": "https://github.com/kjirou/escape-from-the-maze#readme",
  "keywords": [
    "cui",
    "game",
    "maze"
  ],
  "license": "MIT",
  "main": "app.es6",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjirou/escape-from-the-maze.git"
  },
  "scripts": {
    "b": "npm run build",
    "build": "gulp build && npm run build-web-test",
    "build-web-test": "npm run copy-mocha-web-runner && gulp build-js-test",
    "browser-test-for-ci": "testem ci -l phantomjs",
    "browser-test-for-local": "testem ci -l phantomjs,chrome,firefox",
    "copy-mocha-web-runner": "cp node_modules/mocha/mocha.css public/dist && cp node_modules/mocha/mocha.js public/dist",
    "lint": "eslint --ext .es6 *.es6 actions components conf consts dispatcher env input lib models stores test",
    "start": "node bin/escape-from-the-maze",
    "test": "mocha 'test/*.es6' 'test/!(support)/**/*.es6'"
  }
}
