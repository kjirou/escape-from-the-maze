    "start": "node bin/escape-from-the-maze",
    "test": "mocha",
    "zip-server": "cd server/aws-lambda && npm i && zip -r ../../tmp/aws-lambda-`date '+%Y%m%d-%H%M%S'`.zip index.js aws-config.json node_modules && cd ../.."
