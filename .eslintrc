{
  "env": {
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "context": true,
    "describe": true,
    "it": true
  },
  "rules": {
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-unused-vars": [
      2,
      {
        "args": "none"
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ]
  }
}
